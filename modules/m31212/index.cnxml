          <m:annotation encoding="StarMath 5.0"> size 12{ {  {3 left (a+b right )}  over  {a+b} } } {}</m:annotation>
        </m:semantics>
      </m:math>
    </para>
    <para id="id11722342">
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mstyle fontsize="12pt">
              <m:mrow>
