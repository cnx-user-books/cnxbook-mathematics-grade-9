    </para>
    <equation id="id11760009">
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mstyle fontsize="12pt">
              <m:mrow>
                <m:mfrac>
                  <m:mrow>
                    <m:mi>a</m:mi>
                    <m:mfenced open="(" close=")">
                      <m:mrow>
                        <m:mi>b</m:mi>
                        <m:mo stretchy="false">−</m:mo>
                        <m:mi>c</m:mi>
                      </m:mrow>
                    </m:mfenced>
                  </m:mrow>
                  <m:mrow>
                    <m:mn>2</m:mn>
                    <m:mfenced open="(" close=")">
                      <m:mrow>
                        <m:mi>b</m:mi>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mi>c</m:mi>
                      </m:mrow>
                    </m:mfenced>
                  </m:mrow>
